form(action="/contactInfo/"+sell._id+"/commercial/sell" method="post")
    section(name="1_Epos_Terminals" default=values._1)
        header EPOS Terminals
        p What brands/model and how many?
        if values._1
            p You said: #{values._1}
        input(type="textarea")
        footer Reply with text
    section(name="2_Ticket_Printers" default=values._2)
        header Ticket Printers
        p What brands/model and how many?
        if values._2
            p You said: #{values._2}
        input(type="textarea")
        footer Reply with number
    section(name="3_Scanners" default=values._3)
        header Scanners
        p What brands/model and how many?
        if values._3
            p You said: #{values._3}
        input(type="textarea")
        footer Reply with text
    section(name="4_Equipment_Location" default=values._4)
        header Equipment Location
        p Where is this equipment located?
        if values._4
            p You said: #{values._4}
        input(type="textarea")
        footer Reply with text
    section(name="5_Inspect" default=values._5)
        header Availability?
        p When is it available?
        if values._5
            p You said: #{values._5}
        input(type="date")
