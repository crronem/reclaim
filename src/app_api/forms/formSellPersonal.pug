form(action="/contactInfo/"+sell._id+"/personal/sell" header="Sell Personal")
    section(name="1_Laptops" default=values._1)
        header Laptops
        p What brands/model and how many?
        if values._1
            p You said: #{values._1}
        input(type="textarea")
        footer Reply with text
    section(name="2_Desktops" default=values._2)
        header Desktops
        p What brands/model and how many?
        if values._2
            p You said: #{values._3}
        input(type="textarea")
        footer Reply with text
    section(name="3_Printers" default=values._3)
        header Printers
        p What brands/model and how many?
        if values._3
            p You said: #{values._3}
        input(type="textarea")
        footer Reply with text
    section(name="4_Storage_Devices" default=values._4)
        header Storage Devices
        p What brands/model and how many?
        if values._4
            p You said: #{values._4}
        input(type="textarea")
        footer Reply with text
    section(name="5_Equipment_Location" default=values._5)
        header Equipment Location
        p Where is this equipment located?
        if values._4 != "Storage"
            p You said: #{values._4}
        input(type="textarea")
        footer Reply with text
    section(name="6_Inspect" default=values._6)
        header Availability?
        p When is it available?
        if values._6
            p You said: #{values._6}
        input(type="date")